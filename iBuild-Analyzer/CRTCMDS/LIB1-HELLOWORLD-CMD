&USER     = 'DMAGID_X'
&CURR_DIR = '/home/dmagid_x'
&HOME_DIR = '/home/'
CHGLIBL LIBL(DMDMOP1OBJ QGPL QTEMP)
HELLOWORLD.CMD : QCMDSRC.FILE/HELLOWORLD.MBR
ifdef LIB1-HELLOWORLD-CMD-PARMS
	$(eval @_PARMS = $(LIB1-HELLOWORLD-CMD-PARMS))
else
	$(eval @_PARMS = $(CRTCMDPARMS))
endif
	system -v -k "CRTCMD $(@_PARMS) CMD($(LIB1)/HELLOWORLD) PGM($(LIB1)/HELLOWORLD) SRCFILE(QCMDSRC) SRCMBR(HELLOWORLD) TEXT('Hello World command definition') MODE(*PROD *DEBUG *SERVICE) ALLOW(*BPGM *IPGM *EXEC *INTERACT *BATCH *BREXX *IREXX *BMOD *IMOD) CURLIB(*NOCHG) PRDLIB(*NOCHG)"

